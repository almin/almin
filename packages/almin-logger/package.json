{
  "name": "almin-logger",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/almin/almin.git"
  },
  "author": "azu",
  "email": "azuciao@gmail.com",
  "homepage": "https://github.com/almin/almin/tree/master/packages/almin-logger",
  "license": "MIT",
  "files": [
    "src/",
    "lib/"
  ],
  "bugs": {
    "url": "https://github.com/almin/almin/issues"
  },
  "version": "4.1.0",
  "description": "logger for Almin.js",
  "main": "lib/AlminLogger.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "NODE_ENV=production babel src --out-dir lib --source-maps",
    "watch": "babel src --out-dir lib --watch --source-maps",
    "prepublish": "npm run --if-present build",
    "test:saucelabs": "zuul -- test/*-test.js",
    "test:browser": "zuul --local 8080 --ui mocha-bdd -- test/*-test.js",
    "test": "mocha",
    "ci": "npm run test"
  },
  "keywords": [
    "almin",
    "logger",
    "log"
  ],
  "devDependencies": {
    "almin": "^0.13.0",
    "babel-cli": "^6.7.7",
    "babel-loader": "^6.2.10",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015-ie": "^6.6.2",
    "babel-preset-jsdoc-to-assert": "^4.0.0",
    "babel-preset-power-assert": "^1.0.0",
    "babel-register": "^6.7.2",
    "babelify": "^7.3.0",
    "mocha": "^3.0.1",
    "power-assert": "^1.3.1",
    "simple-mock": "^0.7.0",
    "webpack": "^2.2.1",
    "zuul": "^3.10.1"
  },
  "peerDependencies": {
    "almin": ">= 0.13.0"
  },
  "dependencies": {
    "map-like": "^2.0.0"
  }
}
