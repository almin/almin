{
  "filters": {
    "comments": true
  },
  "rules": {
    "prh": {
      "rulePaths": [
        "prh.yml"
      ]
    },
    "alex": {
      "allow": [
        "execute",
        "executed",
        "execution",
        "failure",
        "pros",
        "color"
      ]
    },
    "no-dead-link": {
      "checkRelative": true,
      "ignore": [
        "https://vsavkin.com/*"
      ]
    },
    "common-misspellings": true
  }
}
