{
  "name": "svg-feeling",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/azu/svg-feeling.git"
  },
  "author": "azu",
  "email": "azuciao@gmail.com",
  "homepage": "https://github.com/azu/svg-feeling",
  "license": "MIT",
  "files": [
    "src/",
    "lib/"
  ],
  "bugs": {
    "url": "https://github.com/azu/svg-feeling/issues"
  },
  "version": "1.0.0",
  "description": "SVG feeling",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "clean": "rimraf public/build && mkdirp public/build",
    "start": "npm-run-all clean --parallel watch server",
    "server": "webpack-dev-server --hot --content-base public/ --open",
    "build:js": "NODE_ENV=production webpack -p",
    "build:css": "postcss --config postcss.config.js",
    "build": "npm-run-all clean --parallel build:*",
    "watch:js": "NODE_ENV=development webpack -d --watch",
    "watch:css": "chokidar --initial 'src/**/*.css' -c 'postcss --config postcss.config.js'",
    "watch": "npm-run-all clean --parallel watch:*",
    "prepublish": "npm run --if-present build",
    "test": "mocha",
    "ci": "rimraf node_modules && npm i --ignore-scripts && npm test && webpack"
  },
  "keywords": [
    "svg"
  ],
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-jsdoc-to-assert": "^4.0.0",
    "babel-preset-power-assert": "^1.0.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.7.2",
    "chokidar-cli": "^1.2.0",
    "gh-pages": "^0.12.0",
    "json-loader": "^0.5.4",
    "mkdirp": "^0.5.1",
    "mocha": "^3.0.2",
    "npm-run-all": "^4.0.1",
    "postcss-calc": "^5.2.0",
    "postcss-cli": "^2.5.1",
    "postcss-custom-media": "^5.0.1",
    "postcss-custom-properties": "^5.0.0",
    "postcss-easy-import": "^2.0.0",
    "power-assert": "^1.3.1",
    "rimraf": "^2.5.2",
    "suitcss-preprocessor": "^3.0.1",
    "webpack": "^2.0.0",
    "webpack-dev-server": "^2.4.3"
  },
  "dependencies": {
    "almin": "file:../../",
    "almin-logger": "^4.0.0",
    "babel-runtime": "^6.6.1",
    "classnames": "^2.2.3",
    "hex-and-rgba": "^1.0.0",
    "lodash.range": "^3.1.4",
    "prop-types": "^15.5.4",
    "randomcolor": "^0.4.4",
    "react": "^15.5.3",
    "react-addons-perf": "^15.0.1",
    "react-addons-shallow-compare": "^15.0.1",
    "react-dom": "^15.5.3",
    "suitcss-base": "^2.0.0",
    "suitcss-classnames": "^1.1.0",
    "uuid": "^3.0.1"
  }
}
